{
  "name": "auth",
  "version": "0.1.0",
  "description": "An authentication system",
  "main": "src/index.ts",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --exec node -r @swc-node/register --env-file=.env src/index.ts",
    "debug": "node --inspect-brk -r @swc-node/register --env-file=.env src/index.ts",
    "build": "tsc",
    "start": "node ./dist/index.js",
    "lint": "eslint . --fix",
    "db:push": "prisma db push"
  },
  "keywords": [
    "authentication",
    "auth"
  ],
  "dependencies": {
    "@fastify/auth": "^4.6.1",
    "@fastify/bearer-auth": "^9.4.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.1.0",
    "@prisma/client": "5.15.0",
    "@scalar/fastify-api-reference": "^1.24.8",
    "date-fns": "^3.6.0",
    "envalid": "^8.0.0",
    "fastify": "^4.27.0",
    "fastify-type-provider-zod": "^1.2.0",
    "pino": "^9.1.0",
    "pino-pretty": "^11.1.0",
    "prisma-json-types-generator": "^3.0.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^20.1.3",
    "@types/node": "^20.14.2",
    "eslint-config": "workspace:*",
    "prisma": "^5.15.0"
  }
}
